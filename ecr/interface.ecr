module <%= namespace_name %>
  <% render_doc(object) -%>
  module <%= type_name %>
    <% render_properties %>
    <% render_methods %>
    <% render_signals %>

    abstract def to_unsafe
  end

  # :nodoc:
  @[GObject::GeneratedWrapper]
  class <%= type_name %>__Impl < GObject::Object
    include <%= type_name %>

    <% render "ecr/g_type_method.ecr" %>
    <% render "ecr/cast_methods.ecr" %>
    <% render "ecr/ref_count_method.ecr" %>
  end
end